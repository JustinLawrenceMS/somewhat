{
  "name": "somewhat",
  "version": "1.0.0",
  "description": "helper that allows you to wildcard search recursively through a large js object such as window or document",
  "main": "dist/cjs/SomewhatEffective.js",
  "module": "dist/esm/SomewhatEffective.js", 
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build:cjs": "babel src --out-dir dist/cjs --presets=@babel/preset-env",
    "build:esm": "babel src --out-dir dist/esm --presets=@babel/preset-env --no-babelrc",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinLawrenceMS/somewhat.git"
  },
  "keywords": [
    "search object",
    "object enumeration"
  ],
  "author": "Justin Lawrence, MS",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/JustinLawrenceMS/somewhat/issues"
  },
  "homepage": "https://github.com/JustinLawrenceMS/somewhat#readme",
  "dependencies": {
    "babel-plugin-transform-commonjs": "^1.1.6",
    "jest": "^29.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
